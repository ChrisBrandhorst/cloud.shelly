{
  "type": "mdns-sd",
  "mdns-sd": {
    "name": "http",
    "protocol": "tcp"
  },
  "id": "{{txt.id}}",
  "conditions": [
    [
      {
        "field": "txt.id",
        "match": {
          "type": "regex",
          "value": "^shelly4pro-"
        }
      }
    ],
    [
      {
        "field": "host",
        "match": {
          "type": "regex",
          "value": "^shelly4pro-"
        }
      }
    ]
  ]
}
