  "pair": [
    {
      "id": "start",
      "navigation": {
        "next": "cloud_pairing_form"
      }
    },
    {
      "id": "cloud_pairing_form",
      "$template": "cloud_pairing_form"
    },
    {
      "id": "add_device",
      "$template": "add_device"
    }
  ],
  "energy": {
    "batteries": [
      "CR123A",
      "CR123A"
    ]
  }
