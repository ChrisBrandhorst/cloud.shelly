{
  "name": {
    "en": "Shelly EM",
    "nl": "Shelly EM"
  },
  "images": {
    "large": "./assets/drivers/shellyem/images/large.jpg",
    "small": "./assets/drivers/shellyem/images/small.jpg"
  },
  "class": "socket",
  "$extends": [
    "defaults_cloud"
  ],
  "capabilities": [
    "onoff",
    "measure_power",
    "measure_voltage",
    "meter_power",
    "meter_power_returned",
    "rssi"
  ],
  "pair": [
    {
      "id": "login_oauth2",
      "template": "login_oauth2",
      "options": {
        "hint": "Log in with your Shelly account in the new window.",
        "button": "Log In"
      }
    },
    {
      "id": "list_devices",
      "template": "list_devices",
      "options": {
        "singular": true
      },
      "navigation": {
        "next": "add_device"
      }
    },
    {
      "id": "add_device"
    }
  ],
  "energy": {
    "cumulative": true
  }
}
